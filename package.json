{
  "name": "ai-prompt-organizer",
  "version": "0.1.7",
  "description": "Super simple and scalable AI prompt management tools",
  "keywords": [
    "ai",
    "prompt",
    "organizer",
    "inject",
    "variables",
    "liquid"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "ryanjyost",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/ryanjyost/ai-prompt-organizer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ryanjyost/ai-prompt-organizer.git"
  },
  "bugs": {
    "url": "https://github.com/ryanjyost/ai-prompt-organizer/issues"
  },
  "scripts": {
    "dev": "tsc-watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "xxxprepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "bump:patch": "npm version patch"
  },
  "dependencies": {
    "@types/node": "^20.5.9",
    "chalk": "^5.3.0",
    "glob": "^10.3.4",
    "liquidjs": "^10.9.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tsc-watch": "^6.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.2.2"
  }
}
